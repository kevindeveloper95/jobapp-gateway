# Environment Configuration for Gateway Service
NODE_ENV=development
ENABLE_APM=0

# JWT Tokens
JWT_TOKEN=your_jwt_token_here
GATEWAY_JWT_TOKEN=your_gateway_jwt_token_here

# Secret Keys
SECRET_KEY_ONE=your_secret_key_one_here
SECRET_KEY_TWO=your_secret_key_two_here

# Client URL
CLIENT_URL=http://localhost:3000

# Service URLs
AUTH_BASE_URL=http://localhost:4001
USERS_BASE_URL=http://localhost:4002
GIG_BASE_URL=http://localhost:4003
MESSAGE_BASE_URL=http://localhost:4004
ORDER_BASE_URL=http://localhost:4005
REVIEW_BASE_URL=http://localhost:4006

# Redis Configuration
REDIS_HOST=redis://username:password@localhost:6379

# Elasticsearch Configuration
ELASTIC_SEARCH_URL=http://localhost:9200

# APM Configuration (if enabled)
ELASTIC_APM_SERVER_URL=http://localhost:8200
ELASTIC_APM_SECRET_TOKEN=your_apm_secret_token 